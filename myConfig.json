{
  "name": "yo",
  "inputs": ["name:My KMonad output"],
  "debug": ["listDevices", "logStuff"],
  "combos": [
    {
      "from": ["s", "d"],
      "to": ["leftshift"]
    },
    {
      "from": ["k", "l"],
      "to": ["rightshift"]
    },
    {
      "from": ["s", "f"],
      "to": ["leftctrl"]
    },
    {
      "from": ["j", "l"],
      "to": ["rightctrl"]
    },
    {
      "from": ["j", "k", "l"],
      "to": ["rightctrl", "rightshift"]
    },
    {
      "from": ["s", "d", "f"],
      "to": ["leftctrl", "leftshift"]
    },
    {
      "from": ["q", "w"],
      "to": ["esc"]
    },
    {
      "from": ["v", "i"],
      "to": ["leftmeta", "v"]
    },
    {
      "from": ["g", "h"],
      "to": ["backspace"]
    }
  ]
